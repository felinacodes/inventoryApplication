<div id="passwordPrompt" class="hidden">
    <label for="password" class="mr-2">Password:</label>
    <input type="password" id="password" name="password" autocomplete="current-password"
        class="px-2 py-1 rounded focus:outline-none focus:ring-2 focus:ring-orange-300 font-serif dark:text-black">
    <button type="submit" class="ml-2 px-4 py-1 bg-primary-dark text-white rounded font-serif hover:bg-secondary">Submit</button>
</div>